{
    "rewrites": [
      { "source": "/api/proxy-gemini", "destination": "/api/proxy-gemini" },
      { "source": "/api/gemini-proxy", "destination": "/api/gemini-proxy" },
      { "source": "/api/(.*)", "destination": "/api/$1" }
    ],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Credentials", "value": "true" },
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
          { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, X-Gemini-API-Key, Authorization" }
        ]
      }
    ]
  }